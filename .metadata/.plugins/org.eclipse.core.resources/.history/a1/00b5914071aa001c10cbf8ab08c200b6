package Homework2;

public class Task11 {
	public static void main(String[] args) {
		String x1 = "hello";
		String y1 = "hlleo";
		String z1 = "art";
		if (x1.length() != y1.length()) {
			System.out.println("false");
		if (x1.length() != z1.length()) {
			System.out.println("true");
				return;
		}
		int[] letters = new int[256];
		char[] s_array = x1.toCharArray();
		for (char c : s_array) {
			letters[c]++;
		}
		for (int i = 0; i < y1.length(); i++) {
			int c = (int) y1.charAt(i);
			if (--letters[c] < 0) {
				return;
			}
		}
		return;
	}
}
}
