import java.util.Scanner;

public class TASK1 {

	public class task3 {
		public static void main(String[] args) {
			Scanner scanner = new Scanner(System.in);

			System.out.println("Введите цифру от 1 до 10:");
			int x = scanner.nextInt();
			int y = 1;
			int z;

			while (y < 11) {
				z = x * y;
				System.out.println(x + " * " + y + " = " + z);
				y++;
			}
		}
	}
}
		
	