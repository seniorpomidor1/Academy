import java.util.Scanner;
public class Task2 {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
    System.out.print("Введите число: ");
    String s1 = scanner.nextLine();
	switch (s) {
	case "int":
		System.out.println("Введите число: ");
		int s2 = scanner.nextInt();
		s2 = s2%2;
		System.out.println("Остаток деления на 2");
		break;
	case "double":
		System.out.println("Введите число: ");
		double s3 = scanner.nextDouble();
		s3 = s3 * 0.7;
	System.out.println("Вывести *0.7");
	break;
	case "float":
		System.out.println("Введите число: ");
		float s4 = scanner.nextFloat();
		s4 = (float) Math.pow(s4, 2);
	System.out.println("Возвести в квадрат");
	break;
	case "char":
		System.out.println("Введите символ: ");
		char s5 = scanner.next().charAt(0);
		int x = (int) s5;
	System.out.println("Вывести код символа");
	break;
	case "String":
		System.out.println("Введите Ваше имя:");
		String s6 = scanner.nextLine();
		System.out.println("Привет, " + s6 + "!");
	System.out.println("Hello" + "a");
	break;
	case "other":
	System.out.println("Unsupported type");
	break;
	}
}
}